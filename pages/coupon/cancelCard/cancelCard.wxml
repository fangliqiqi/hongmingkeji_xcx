<!--pages/coupon/cancelCard/cancelCard.wxml-->
<view class="page">
  <view class="flex flex-col flex-aic info">
    <view class="fs-36">{{discountInfo.businessName}}</view>
    <view class="fs-28 mt-10 col5e5f66 ">有效期至{{discountInfo.endTime}}</view>
    <view class="code mw0-30 ml-a mr-a mt-50">  
      <view class="fs-28">券码</view>
      <view class="fs-28">{{code}}</view>
      <view class="ewm">
        <image src="{{scanCode}}" class="codeimg"></image>
        <view class="cancel">
          <view class="text fs-26">
            <text>{{discountInfo.chargeOffTime}}</text>
            <text>已核销</text>
          </view>   
        </view>
      </view>
    </view>
    <!-- <view class="bgact btn mt-30">分享得次数</view> -->
  </view>
</view>
