/* pages/coupon/cancelCard/cancelCard.wxss */
.page{
  background: #ffffff;
}
.info{
  padding-top: 80rpx;
}
.code{
  padding:30rpx;
  box-sizing: border-box;
  background:#f1f1f1;
  border-radius:6rpx;
}
.ewm{
  width:440rpx;
  height:440rpx;
  margin:38rpx auto 0;
  position:relative;
}
.codeimg{
  width:100%;
  height:100%;
}
.cancel{
  position: absolute;
  top:0;
  left:0;
  border-radius: 10rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  width:100%;
  height: 100%;
  background:rgba(0, 0, 0, 0.3);
} 
.text{
  display:flex;
  width:280rpx;
  height:280rpx;
  border-radius:50%;
  background:#ffffff;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.btn{
  width:200rpx;
  height: 80rpx;
  text-align: center;
  line-height: 80rpx;
  color:#ffffff;
  font-size:28rpx;
  border-radius: 10rpx;
}