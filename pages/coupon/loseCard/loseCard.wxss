/* pages/coupon/loseCard/loseCard.wxss */
.page{
  background: #ffffff;
}
.loseCard{
  padding: 30rpx 0;
  box-sizing: border-box;
}
.card_bg{
  width:100%;
  height:198rpx;
  border-radius: 10rpx;
  overflow: hidden;
  padding:0 40rpx;
  box-sizing: border-box;
  position: relative;
  display: flex;
  justify-content: space-between;
  align-items: center;
  background:#f1f1f1;
}
.card_img{
  width:100%;
  height:100%;
  position:absolute;
  top: 0;
  left: 0;
}
.card_left{
  z-index: 999;
}
.card_right{
  z-index: 999;
}
.btn{
  width:160rpx;
  height:60rpx;
  background:rgba(255, 255, 255, 0.8);
  border-radius:40rpx;
  text-align: center;
  line-height: 60rpx;
}
.no{
  width:100%;
  height: 100vh;
  text-align: center;
  line-height: 60vh;
}

.clear_btn{
  width:610rpx !important;
  height: 90rpx;
  background: #ffffff;
  border:1rpx solid #f1f1f1;
  border-radius: 10rpx;
}