/* pages/wallet/expenseDetail/expenseDetail.wxss */
.page{
  background: #fff;
}
.pad30{
  padding:30rpx;
  box-sizing: border-box;
}
.order{
  border: 1rpx solid #e5e5e5;
  border-radius:6rpx;
  padding:20rpx;
  box-sizing: border-box;
}
.card{
  width:100%;
  height:198rpx;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
}
.card_img{
  width:100%;
  height:100%;
  position:absolute;
  top: 0;
  left: 0;
}
.card_name{
  z-index: 999;
}
.colorGray{
  color:#49576f;
}
.colorCyan{
  color:#43737e;
}
.colorYellow{
  color:#855e3b;
}
.colorBlue{
  color:#425b82;
}
.flex_left{
  flex:0 0 140rpx;
  text-align: right;
}
.flex_con{
  flex:0 0 100rpx;
  text-align:center;
}
.flex_right{
  flex:1;
  text-align: left;
}
.eq_left{
  flex:0 0 120rpx;
  text-align: center;
}
.bus_left{
  flex:0 0 300rpx;
  text-align: left;
}
.bus_right{
  flex:1;
  text-align: left;
}

.btn{
  width:140rpx;
  height: 60rpx;
  color:#ffffff;
  text-align: center;
  line-height: 60rpx;
  border-radius: 8rpx;
}